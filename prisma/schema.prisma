generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model CustomerServiceData {
  id                    Int      @id @default(autoincrement())
  period                String
  complaints           Int
  contactRequests      Int
  maintenanceRequests  Int
  inquiries            Int
  officeInterested     Int
  projectsInterested   Int
  customersInterested  Int
  generalInquiries     Int
  documentRequests     Int
  deedInquiries        Int
  apartmentRentals     Int
  soldProjects         Int
  maintenanceCancelled Int
  maintenanceResolved  Int
  maintenanceInProgress Int
  createdAt            DateTime @default(now())
}